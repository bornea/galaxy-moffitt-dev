prefix=/galaxy/builds/Linux-x86_64/install_dir
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
sysconfdir=${prefix}/etc
localstatedir=${prefix}/var
PACKAGE=fontconfig
confdir=${sysconfdir}/fonts
cachedir=${localstatedir}/cache/${PACKAGE}

Name: Fontconfig
Description: Font configuration and customization library
Version: 2.11.1
Requires:  freetype2
Requires.private:  libxml-2.0
Libs: -L${libdir} -lfontconfig
Libs.private:  -L/var/galaxy/build_r/install_dir/lib -lfreetype   -L/galaxy/builds/Linux-x86_64/install_dir/lib -lxml2 
Cflags: -I${includedir}  -I/galaxy/builds/Linux-x86_64/install_dir/include/freetype2  -I/galaxy/builds/Linux-x86_64/install_dir/include/libxml2 
